{
  "baseAccount": "your-github-username",
  "type": "user",
  "website": {
    "title": "Your Name - Developer",
    "favicon": "/favicon.svg"
  },
  "typography": {
    "name": {
      "enabled": false,
      "fontSize": "text-5xl",
